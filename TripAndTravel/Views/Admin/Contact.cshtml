@using TripAndTravel.Models
@model List<Contact>
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>İletişim</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>İSİM</th>
        <th>MAİL</th>
        <th>KONU</th>
        <th>MESAJ</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.AdSoyad</td>
            <td>@item.Mail</td>
            <td>@item.Konu</td>
            <td>@item.Mesaj</td>
        </tr>
    }
</table>

